	"tokenColors": [
		{
			"name": "Primary/Foreground",
			"scope": [
				"source",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#8F8F8A"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5A5A59",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment Doc",
			"scope": [
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#878782"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#DA7C7E"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#AA9DCA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"support.type",
				"storage.type.cs",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#D1A075"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#A0A973"
			}
		},
		{
			"name": "Number / Constant",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#D1A075"
			}
		},
		{
			"name": "Boolean",
			"scope": [
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#CE94A7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant (Zed Special)",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#3B9174",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.other.object"
			],
			"settings": {
				"foreground": "#C0BEAF"
			}
		},
		{
			"name": "Property",
			"scope": [
				"variable.other.property",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AA9DCA"
			}
		},
		{
			"name": "Operator / Punctuation",
			"scope": [
				"keyword.operator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#878782"
			}
		},
		{
			"name": "Brackets / Delimiters",
			"scope": [
				"punctuation.section",
				"punctuation.terminator",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#8F8F8A"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#CE94A7",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#D1A075"
			}
		},
		{
			"name": "Namespace / Module",
			"scope": [
				"entity.name.namespace",
				"storage.modifier.import",
				"storage.modifier.package"
			],
			"settings": {
				"foreground": "#AA9DCA"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#AA9DCA"
			}
		},
		{
			"name": "Title / Heading",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#3B9174",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Rust Attributes",
			"scope": [
				"entity.name.type.content.rust",
				"meta.attribute.rust",
				"punctuation.definition.attribute.rust",
				"storage.modifier.attribute.rust",
				"entity.other.attribute-name.rust"
			],
			"settings": {
				"foreground": "#CE94A7",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Rust Cargo Doc / Documentation Comments",
			"scope": [
				"comment.line.documentation.rust",
				"comment.block.documentation.rust",
				"punctuation.definition.comment.rust"
			],
			"settings": {
				"foreground": "#878782"
			}
		},
		{
			"name": "Rust Macro",
			"scope": [
				"entity.name.function.macro.rust",
				"support.function.macro.rust",
				"variable.language.special.rust"
			],
			"settings": {
				"foreground": "#AA9DCA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Rust Lifetime",
			"scope": [
				"storage.modifier.lifetime.rust",
				"entity.name.type.lifetime"
			],
			"settings": {
				"foreground": "#D1A075"
			}
		},
		{
			"name": "Rust Constants & Enums",
			"scope": [
				"entity.name.constant.rust",
				"variable.other.constant.rust",
				"entity.name.type.enum.rust"
			],
			"settings": {
				"foreground": "#3B9174",
				"fontStyle": "bold"
			}
		}
	],
}